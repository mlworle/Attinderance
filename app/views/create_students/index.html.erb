<p id="notice"><%= notice %></p>

<h1>Listing Create Students</h1>

<table>
  <thead>
    <tr>
      <th>First name</th>
      <th>Last name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @create_students.each do |create_student| %>
      <tr>
        <td><%= create_student.first_name %></td>
        <td><%= create_student.last_name %></td>
        <td><%= link_to 'Show', create_student %></td>
        <td><%= link_to 'Edit', edit_create_student_path(create_student) %></td>
        <td><%= link_to 'Destroy', create_student, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Create student', new_create_student_path %>
